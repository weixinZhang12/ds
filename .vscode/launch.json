            "cargo": {
                "args": [
                    "build",
                    "--bin=ds",
                    "--package=ds"
                ],
                "filter": {
                    "name": "ds",
                    "kind": "bin"
                }
            },
            "expressions": "simple",
            "preRunCommands": [
                "command script import ${workspaceFolder}/.vscode/rust_prettifier_for_lldb.py"
            ],
            "args": [],
            "cwd": "${workspaceFolder}"
        },
